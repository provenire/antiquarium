<!-- START JUMBOTRON -->
<div class="jumbotron no-margin" data-pages="parallax">
  <div class="container-fluid container-fixed-lg sm-p-l-20 sm-p-r-20">
    <div class="inner">
      <h3>Explore <%= title %></h3>
    </div>
  </div>
</div>
<!-- END JUMBOTRON -->

<div class="container-fluid container-fixed-lg">

  <!-- START BREADCRUMB -->
  <ul class="breadcrumb">
    <li>
      <p><%= title %></p>
    </li>
    <li><span class="active"><%= page_entries_info objects %></span>
    </li>
  </ul>
  <!-- END BREADCRUMB -->

  <div class="dataTables_wrapper">
  <div class="table-responsive">
    <table class="table m-b-0">
      <thead>
        <th style="width:10%;padding-top:0;padding-bottom:0;"></th>
        <th style="width:30%;padding-top:0;padding-bottom:0;"></th>
        <th style="width:45%;padding-top:0;padding-bottom:0;"></th>
        <th style="width:15%;padding-top:0;padding-bottom:0;"></th>
      </thead>
      <tbody>

        <%= render objects %>
      </tbody>
    </table>
  </div>

  <div class="row m-b-20">
    <div class="dataTables_paginate paging_bootstrap pagination">
      <%= paginate objects, :views_prefix => 'templates/' %>
    </div>
    <div class="dataTables_info">
      Showing <%= page_entries_info objects %> entries
    </div>
  </div>
</div>

</div>
